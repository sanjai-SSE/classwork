#include <stdio.h>
int main() {
    int a[N][N] = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};
    int i, j, sum = 0;

    for (i = 0; i < N; i++) {
        for (j = 0; j < N; j++) {
            if (i == j) {
                sum += a[i][j];
            }
        }
    }

    printf("The sum of the diagonal elements is %d\n", sum);

    return 0;
}
